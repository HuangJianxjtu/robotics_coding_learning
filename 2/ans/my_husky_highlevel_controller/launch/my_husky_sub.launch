<launch>
    <include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
        <arg name="world_name" value="worlds/robocup14_spl_field.world"/>
    </include>
    <node name="teleop_twist_keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen"/>
    <node name="my_husky_sub" pkg="my_husky_highlevel_controller" type="my_husky_sub" output="screen">
        <rosparam file="$(find my_husky_highlevel_controller)/config/config.yaml" command="load"/>
    </node>
</launch>